    "subject = [\"Python\", \"C++\", \"Java\"]\n",
    "marks = [20, 10 , 5]\n",
    "\n",
    "fig, ax = plt.subplots()\n",
    "ax.pie(marks, labels = subject, autopct='%1.1f%%')\n",
    "st.pyplot(fig)"
